{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/*.js",
          "/*.css",
          "/manifest.webmanifest"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/favicon.ico"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-images",
      "urls": [
        "https://image.pollinations.ai/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 50,
        "maxAge": "7d",
        "timeout": "30s"
      }
    },
    {
      "name": "api-text",
      "urls": [
        "https://text.pollinations.ai/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "1h",
        "timeout": "15s"
      }
    },
    {
      "name": "api-models",
      "urls": [
        "https://image.pollinations.ai/models",
        "https://text.pollinations.ai/models"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 5,
        "maxAge": "1d",
        "timeout": "10s"
      }
    }
  ]
}
